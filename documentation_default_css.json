{
  "body": {
    "background-color": "black",
    "color": "white",
    "font-family": "Segoe UI, SegoeUI, Helvetica Neue, Helvetica, Arial, sans-serif",
    "padding": "0 5%"
  },
  "a, a:active, a:visited, a:hover": {
    "color": "rgb(117, 182, 231)",
    "text-decoration": "none"
  },
  "a small": {
    "color": "rgb(60, 90, 115)"
  },
  "h1, h2, h3, p": {
    "margin-left": "0",
    "margin": "0.5em"
  },
  ".deprecated h4": {
    "margin": "0",
    "margin-bottom": "0.5em"
  },
  "div.deprecated": {
    "background-color": "rgb(99, 0, 1)",
    "padding": "0 0.5em",
    "width": "fit-content"
  },
  "a.deprecated": {
    "text-decoration": "line-through"
  },
  "h1": {
    "font-size": "200%"
  },
  "h2": {
    "font-size": "150%"
  },
  "summary.h2": {
    "margin-left": "0",
    "margin": "0.5em",
    "font-size": "150%",
    "font-weight": "bold"
  },
  "h3": {
    "margin-top": "0.5em",
    "margin-bottom": "0"
  },
  "p": {
    "font-size": "10pt"
  },
  "table": {
    "width": "100%",
    "border-collapse": "collapse"
  },
  "tr": { "border": "1px solid gray" },
  "td": {
    "padding": "5px"
  },
  "td.trivial": {
    "color": "gray"
  },
  "td md p": {
    "margin": "0"
  },
  ".desclist": {
    "font-size": "9pt",
    "color": "lightgray",
    "padding-left": "0"
  },
  "li": {
    "margin-left": "0",
    "list-style-type": "none"
  },
  "dl": {
    "margin": "0"
  },
  "dt": {
    "font-size": "120%"
  },
  "dd": {
    "margin-left": "0"
  },
  ".declnamecol": {
    "width": "0;",
    "min-width": "max-content",
    "padding-right": "1em"
  },
  ".enumnamecol": {
    "width": "0;",
    "min-width": "max-content",
    "padding-right": "1em"
  },
  ".enumvalcol": {
    "width": "0",
    "min-width": "max-content",
    "font-family": "Consolas, Courier, monospace"
  },
  ".entitylink": {
    "font-family": "Consolas, Courier, monospace"
  },
  ".membertype": {
    "color": "gray",
    "font-family": "Consolas, Courier, monospace"
  },
  "pre": {
    "display": "inline",
    "margin": "0"
  },
  "code": {
    "margin": "0",
    "background-color": "rgb(40,40,40)",
    "display": "inline !important",
    "padding": "0 !important"
  },
  "code.example": {
    "font-size": "100%",
    "border": "1px solid gray",
    "padding": "0.5em !important",
    "display": "block !important"
  },
  "see": {
    "font-family": "Consolas, Courier, monospace",
    "background-color": "rgb(40,40,40)"
  },
  ".paramname": {
    "font-weight": "bold"
  }
}