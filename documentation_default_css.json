{
  "body": {
    "background-color": "black",
    "color": "white",
    "font-family": "Segoe UI, SegoeUI, Helvetica Neue, Helvetica, Arial, sans-serif",
    "padding": "0 5%"
  },
  "a, a:active, a:visited, a:hover": {
    "color": "rgb(117, 182, 231)",
    "text-decoration": "none"
  },
  "a small": {
    "color": "rgb(60, 90, 115)"
  },
  "h1, h2, h3, p": {
    "margin-left": "0",
    "margin": "0.5em"
  },
  "h1": {
    "font-size": "200%"
  },
  "h2": {
    "font-size": "150%"
  },
  "h3": {
    "margin-top": "0.5em"
  },
  "p": {
    "font-size": "10pt"
  },
  "pre": {
    "margin": "0"
  },
  "table": {
    "width": "100%",
    "border-collapse": "collapse"
  },
  "tr": {
    "border": "1px solid gray"
  },
  "td": {
    "padding": "5px"
  },
  "td md p": {
    "margin": "0"
  },
  ".desclist": {
    "font-size": "9pt",
    "color": "lightgray",
    "padding-left": "0"
  },
  "li": {
    "margin-left": "0",
    "list-style-type": "none"
  },
  ".declnamecol": {
    "width": "33%"
  },
  ".entitylink": {
    "font-family": "Consolas, Courier, monospace"
  },
  ".membertype": {
    "color": "gray",
    "font-family": "Consolas, Courier, monospace"
  },
  "code.example": {
    "font-size": "100%",
    "border": "1px solid gray",
    "background-color": "rgb(40,40,40)",
    "padding": "0.5em",
    "display": "block"
  }
}